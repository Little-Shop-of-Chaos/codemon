{
	"version": 2,
	"builds": [
		{
			"src": "server.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/public/(.*)",
			"dest": "/public/$1"
		},
		{
			"src": "/managers/(.*)",
			"dest": "/managers/$1"
		},
		{
			"src": "/assets/(.*)",
			"dest": "/public/assets/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/server.js"
		}
	]
}
